{
	"usesTypeScript": true,
	"usesCssModule": true,
	"cssPreprocessor": "scss",
	"testLibrary": "Testing Library",
	"component": {
		"default": {
			"customTemplates": {
				"component": "templates/default/TemplateName.tsx",
				"style": "templates/default/TemplateName.module.scss",
				"index": "templates/default/index.ts",
				"hook": "templates/default/useTemplateName.ts"
			},
			"path": "components/modules",
			"withLazy": false,
			"withStory": false,
			"withStyle": true,
			"withTest": false,
			"withIndex": true,
			"withHook": true
		},
		"page": {
			"customTemplates": {
				"component": "templates/default/TemplateName.tsx",
				"style": "templates/default/TemplateName.module.scss",
				"index": "templates/default/index.ts",
				"hook": "templates/default/useTemplateName.ts"
			},
			"path": "components/screens",
			"withLazy": false,
			"withStory": false,
			"withStyle": true,
			"withTest": false,
			"withIndex": true,
			"withHook": true
		}
	},
	"usesStyledComponents": false
}
